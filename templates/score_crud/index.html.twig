{% extends 'base.html.twig' %}

{% block title %}Score index{% endblock %}

{% block body %}
    <h1>Score index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>ScoreDossier</th>
                <th>ScoreEtab</th>
                <th>FormuleEtab</th>
                <th>Total</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for score in scores %}
            <tr>
                <td>{{ score.id }}</td>
                <td>{{ score.scoreDossier }}</td>
                <td>{{ score.scoreEtab }}</td>
                <td>{{ score.formuleEtab }}</td>
                <td>{{ score.total }}</td>
                <td>
                    <a href="{{ path('app_score_crud_show', {'id': score.id}) }}">show</a>
                    <a href="{{ path('app_score_crud_edit', {'id': score.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_score_crud_new') }}">Create new</a>
{% endblock %}
