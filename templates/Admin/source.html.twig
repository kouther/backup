<!-- calcul -->
   <script>

      function calcul() {
        let  scoredossier = document.getElementById("score_scoreDossier").value;
        let  scoreEtab = document.getElementById("score_scoreEtab").value;
        let  num1= Number(scoreEtab);
        let  num2= Number(scoredossier);
        let  total= (num1+num2) /2;    
        document.getElementById("score_total").value = total;
        

      }

    </script>
